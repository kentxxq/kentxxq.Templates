{
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "kentxxq.Templates.Aspnetcore.Webapi",
        "Uri": "http://localhost:5000/healthz"
      }
    ]
  },
  "Authentication": {
    "DefaultScheme": "Bearer", // 没有生效
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "ken"
        ],
        "ValidIssuer": "ken"
      },
      // 下面的Test只是示例，没有在代码中用到
      "Test": {
        "ValidAudiences": [
          "localhost:5000"
        ],
        "ValidIssuer": "test",
        "SigningKeys": [
          {
            "Issuer": "test",
            // 准备一个32长度的acsii码，base64放到下面。因为Microsoft.AspNetCore.Authentication.JwtBearerConfigureOptions.GetIssuerSigningKeys里是这么实现的
            // asdasdasdadadssaaasdasaaasdasaaa
            "Value": "YXNkYXNkYXNkYWRhZHNzYWFhc2Rhc2FhYXNkYXNhYWE="
          }
        ]
      }
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:5000",
  // EnableRedis
  "easycaching": {
    "inmemory": {
      "MaxRdSecond": 0,
      "EnableLogging": false,
      "LockMs": 5000,
      "SleepMs": 300,
      "dbconfig": {
        //"ExpirationScanFrequency": 60,
